<p id="notice"><%= notice %></p>

<h1>Listing Colocs</h1>

<table>
  <thead>
    <tr>
      <th>Adress</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @colocs.each do |coloc| %>
      <tr>
        <td><%= coloc.adress %></td>
        <td><%= link_to 'Show', coloc %></td>
        <td><%= link_to 'Edit', edit_coloc_path(coloc) %></td>
        <td>
          <%= link_to 'Destroy', coloc_path(coloc), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coloc', new_coloc_path %>
