<p id="notice"><%= notice %></p>

<table class="profile">
  <tr>
    <td class="main">
	  <p>
	  <strong>Adress:</strong>
	   <%= @coloc.adress %>
		</p>
    
    

    </td>
    <td class="sidebar round">
    <p>
      <strong>Habitants:</strong>
      <% @users.each do |user| %>
        <% if user.coloc_id == @coloc.id %>
            <li><%= user.nom %> <%= user.prenom %></li>
          <% end %>
        <% end %>
        </p>
    </td>
  </tr>
</table>

<div id="map-canvas" style="height: 200px"></div>

<%= link_to 'Edit', edit_coloc_path(@coloc) %> |
<%= link_to 'Back', colocs_path %>

<script type="text/javascript">
  window.data = <%= @coloc.to_json.html_safe %>;
  window.page = 'coloc';
</script> 
